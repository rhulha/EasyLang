

Fenster fenster = neu Fenster();

publik nichts start()
{
	fenster.setzeTitel("Sokoban");
	fenster.setzeBreite(800);
	fenster.setzeHöhe(600);
	#fenster.zeigeAn();
	
	String[] level = neu String[];
	Zahl feldGröße = 40;
	Zahl spielerX = 0;
	Zahl spielerY = 0;
	
	DateiLeser dl = neu DateiLeser("level01.sokoban");
	dl.fürJedeZeile{
		Zeichen zeile -> 
		String[] parts = line.spalte(":");
		level[parts[0]] = parts[1];
		parts[1].equals("Player"){
			String[] coords = parts[0].spalte("-");
			spielerX = coords[0].toZahl();
			spielerY = coords[1].toZahl();
		}
		
	}
	dl.ende();
	
}

